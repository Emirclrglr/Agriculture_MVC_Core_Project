@model EntityLayer.Concrete.Admin
@{
    ViewData["Title"] = "UpdateAdmin";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Yeni Admin Kaydı</h4>
            <form method="post" class="forms-sample">
                <div class="form-group">
                    <label for="exampleInputName1">Kullanıcı Adı<sup class="text-danger">*</sup></label>
                    <input type="text" class="form-control" asp-for="Username">
                </div>
                <div class="form-group">
                    <label for="exampleInputEmail3">Şifre<sup class="text-danger">*</sup></label>
                    <input type="text" class="form-control" asp-for="Password">
                </div>
                <button type="submit" class="btn btn-outline-info me-2">Kaydet</button>
            </form>
        </div>
    </div>
</div>


